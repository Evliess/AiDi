<!--home.wxml-->
<navigation-bar></navigation-bar>
<scroll-view class="container" style="padding: 4px;" scroll-y type="custom">
  <view class="title">
    <view>AIDI 后台管理</view>
  </view>
  <view class="variations">
    <view class="variation-card" bind:tap="addVip">
      <view class="text">新增会员</view>
    </view>
    <view class="variation-card" bind:tap="chargeVip">
      <view class="text">会员充值</view>
    </view>
    <view class="variation-card" bind:tap="playVip">
      <view class="text">会员划卡</view>
    </view>
    <view class="variation-card" bind:tap="viewVip">
      <view class="text">会员信息</view>
    </view>
    <view class="variation-card" bind:tap="addNonVip">
      <view class="text">散客消费</view>
    </view>
    <view class="variation-card">
      <view class="text">财务一览</view>
    </view>
  </view>
</scroll-view>